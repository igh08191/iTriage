<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/imageButtonMedico"
        android:layout_width="83dp"
        android:layout_height="91dp"
        app:srcCompat="@drawable/btnmedico"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="46dp" />

    <ImageButton
        android:id="@+id/imageAmbulancia"
        android:layout_width="89dp"
        android:layout_height="86dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toEndOf="@+id/imageButtonMedico"
        app:srcCompat="@drawable/ambulance"
        tools:layout_editor_absoluteY="63dp" />

    <ImageButton
        android:id="@+id/imageLocPaciente"
        android:layout_width="83dp"
        android:layout_height="82dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonMedico"
        app:srcCompat="@drawable/dc_loc"
        tools:layout_editor_absoluteX="44dp" />

    <ImageButton
        android:id="@+id/imageCasaInfo"
        android:layout_width="85dp"
        android:layout_height="90dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toEndOf="@+id/imageLocPaciente"
        app:layout_constraintTop_toBottomOf="@+id/imageAmbulancia"
        app:srcCompat="@drawable/casainfo" />
</android.support.constraint.ConstraintLayout>